import java.io.*;
public class Pacadalhin_Rowena
{
private static PrintStream p = System.out;
public static BufferedReader num= new BufferedReader (new
InputStreamReader(System.in));
public static void main(String[] args)throws IOException
{
//Prelim Lecture Grade//
p.print("Enter Prelim Quiz#1: ");
int preq1 = Integer.parseInt(num.readLine());
p.print("Enter Prelim Quiz#2: ");
int preq2 = Integer.parseInt(num.readLine());
p.print("Enter Prelim Quiz#3: ");
int preq3 = Integer.parseInt(num.readLine());
p.print("Enter Prelim Quiz#4: ");
int preq4 = Integer.parseInt(num.readLine());
p.print("Enter Prelim Quiz#5: ");
int preq5 = Integer.parseInt(num.readLine());
double TOTALPQUIZ= (preq1+preq2+preq3+preq4+preq5)/5*.40;
p.println("Total Prelim Quiz is: "+TOTALPQUIZ);p.println("");

//Prelim Class Standing
p.print("Enter Prelim Homework grade: ");
int prehomew = Integer.parseInt(num.readLine());
p.print("Enter Prelim Attendance grade: ");

int preatt = Integer.parseInt(num.readLine());
p.print("Enter Prelim Behavior grade: ");
int prebehave = Integer.parseInt(num.readLine());
p.print("Enter Prelim Seatwork grade: ");
int preseatw = Integer.parseInt(num.readLine());
double TOTALPCLASS = (prehomew+ preatt + prebehave + preseatw)/4*.20;
p.println("Total Lecture Class standing is: "+TOTALPCLASS);p.println("");

//Prelim Lecture Exam
p.print("Enter Prelim Lecture Exam: ");
int PREEXAM = Integer.parseInt(num.readLine());
double TOTALPEXAM = PREEXAM*.40;
p.println("Total Prelim Lecture Exam is: "+TOTALPEXAM);
double TOTALPGRADE = TOTALPQUIZ + TOTALPCLASS + TOTALPEXAM;
p.println("Total Prelim Lecture grade is: "+TOTALPGRADE);p.println("");

//Prelim Laboratory Grade
p.print("Enter Prelim Perfromance Task#1: ");
