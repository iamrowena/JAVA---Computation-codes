p.print("Enter Prelim Perfromance Task#2: ");
int prept2 = Integer.parseInt(num.readLine());
p.print("Enter Prelim Perfromance Task#3: ");
int prept3 = Integer.parseInt(num.readLine());
p.print("Enter Prelim Perfromance Task#4: ");
int prept4 = Integer.parseInt(num.readLine());
double TOTALPT = (prept1 + prept2 + prept3 + prept4)/4*.50;
p.println("Total prelim Performance Task is: "+TOTALPT);p.println("");

