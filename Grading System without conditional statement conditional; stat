int prelabbehave = Integer.parseInt(num.readLine());
p.print("Enter Prelim Seatwork grade: ");
int prelabseatw = Integer.parseInt(num.readLine());
double TOTALPLABCLASS = (prelabatt + prelabbehave + prelabseatw)/3*.20;
p.println("Total Laboratory Class standing is: "+TOTALPLABCLASS);p.println("");

//Prelim Lecture Exam
p.print("Enter Prelim Laboratory Exam: ");
int PRELABEXAM = Integer.parseInt(num.readLine());
double TOTALPLABEXAM = PRELABEXAM*.30;
p.println("Total Prelim Lecture Exam is: "+TOTALPLABEXAM);
double TOTALPLABGRADE = (TOTALPT + TOTALPLABCLASS + TOTALPLABEXAM);
p.println("Prelim Total Laboratory Grade: "+TOTALPLABGRADE);p.println("");

//Midterm Lecture Grade//
p.print("Enter Midterm Quiz#1: ");
int midq1 = Integer.parseInt(num.readLine());
p.print("Enter Midterm Quiz#2: ");
int midq2 = Integer.parseInt(num.readLine());
p.print("Enter Midterm Quiz#3: ");
int midq3 = Integer.parseInt(num.readLine());
p.print("Enter Midterm Quiz#4: ");
int midq4 = Integer.parseInt(num.readLine());
p.print("Enter Midterm Quiz#5: ");
int midq5 = Integer.parseInt(num.readLine());
double TOTALMQUIZ= (midq1 + midq2 + midq3 + midq4 + midq5)/5*.40;
p.println("Total Midterm Quiz is: "+TOTALMQUIZ);p.println("");

//Midterm Lecture Class Standing
p.print("Enter Midterm Homework grade: ");
int midhomew = Integer.parseInt(num.readLine());
p.print("Enter Midterm Attendance grade: ");
int midatt = Integer.parseInt(num.readLine());
p.print("Enter Midterm Behavior grade: ");
int midbehave = Integer.parseInt(num.readLine());
p.print("Enter Midterm Seatwork grade: ");
int midseatw = Integer.parseInt(num.readLine());
double TOTALMCLASS = (midhomew+ midatt + midbehave + midseatw)/4*.20;
p.println("Total Midterm Lecture Class standing is: "+TOTALMCLASS);p.println("");

//Midterm Lecture Exam
p.print("Enter Midterm Lecture Exam: ");
int MIDEXAM = Integer.parseInt(num.readLine());
double TOTALMEXAM = MIDEXAM*.40;
p.println("Total Midterm Lecture Exam is: "+TOTALMEXAM);
double TOTALMGRADE = TOTALMQUIZ + TOTALMCLASS + TOTALMEXAM;
p.println("Total Midterm Lecture grade is: "+TOTALMGRADE);p.println("");

//Midterm Laboratory Grade
p.print("Enter Midterm Perfromance Task#1: ");
int midpt1 = Integer.parseInt(num.readLine());
p.print("Enter Midterm Perfromance Task#2: ");
int midpt2 = Integer.parseInt(num.readLine());
p.print("Enter Midterm Perfromance Task#3: ");
int midpt3 = Integer.parseInt(num.readLine());
p.print("Enter Midterm Perfromance Task#4: ");
int midpt4 = Integer.parseInt(num.readLine());
double TOTALMIDPT = (midpt1 + midpt2 + midpt3 + midpt4)/4*.50;

p.println("Total Midterm Performance Task is: "+TOTALMIDPT);p.println("");

//Midterm Laboratory Class
p.print("Enter Midterm Attendance grade: ");
int midlabatt = Integer.parseInt(num.readLine());
p.print("Enter Midterm Compilation grade: ");
int midlabbehave = Integer.parseInt(num.readLine());
p.print("Enter Midterm Seatwork grade: ");
int midlabseatw = Integer.parseInt(num.readLine());
double TOTALMLABCLASS = (midlabatt + midlabbehave + midlabseatw)/3*.20;
p.println("Total Laboratory Class standing is: "+TOTALMLABCLASS);p.println("");

p.print("Enter Midterm Laboratory Exam: ");
int MIDLABEXAM = Integer.parseInt(num.readLine());
double TOTALMLABEXAM = MIDLABEXAM*.30;
p.println("Total Midterm Lecture Exam is: "+TOTALMLABEXAM);
double TOTALMLABGRADE = (TOTALMIDPT + TOTALMLABCLASS +
TOTALMLABEXAM);
p.println(" Total Midterm Laboratory Grade:"+TOTALMLABGRADE);p.println("");

//Final Lecture Grade//
p.print("Enter Final Quiz#1: ");
int fnlq1 = Integer.parseInt(num.readLine());
p.print("Enter Final Quiz#2: ");
int fnlq2 = Integer.parseInt(num.readLine());
p.print("Enter Final Quiz#3: ");
int fnlq3 = Integer.parseInt(num.readLine());
p.print("Enter Final Quiz#4: ");
int fnlq4 = Integer.parseInt(num.readLine());
p.print("Enter Final Quiz#5: ");

int fnlq5 = Integer.parseInt(num.readLine());
double TOTALFQUIZ= (fnlq1 + fnlq2 + fnlq3 + fnlq4 + fnlq5)/5*.40;
p.println("Total Final Quiz is: "+TOTALFQUIZ);p.println("");

//Midterm Lecture Class Standing
p.print("Enter Final Homework grade: ");
int fnlhomew = Integer.parseInt(num.readLine());
p.print("Enter Final Attendance grade: ");
int fnlatt = Integer.parseInt(num.readLine());
p.print("Enter Final Behavior grade: ");
int fnlbehave = Integer.parseInt(num.readLine());
p.print("Enter Final Seatwork grade: ");
int fnlseatw = Integer.parseInt(num.readLine());
double TOTALFCLASS = (fnlhomew+ fnlatt + fnlbehave + fnlseatw)/4*.20;
p.println("Total Final Lecture Class standing is: "+TOTALFCLASS);p.println("");

