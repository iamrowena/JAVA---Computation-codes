p.println("Total Final Lecture grade is: "+TOTALFGRADE);p.println("");

//Final Laboratory Grade
p.print("Enter Final Performance Task#1: ");
int fnlpt1 = Integer.parseInt(num.readLine());
p.print("Enter Final Performance Task#2: ");
int fnlpt2 = Integer.parseInt(num.readLine());
p.print("Enter Final Performance Task#3: ");

int fnlpt3 = Integer.parseInt(num.readLine());
p.print("Enter Final Performance Task#4: ");
int fnlpt4 = Integer.parseInt(num.readLine());
double TOTALFNLPT = (fnlpt1 + fnlpt2 + fnlpt3 + fnlpt4)/4*.50;
p.println("Total Final Performance Task is: "+TOTALFNLPT);p.println("");

//Final Laboratory Class
p.print("Enter Final Attendance grade: ");
int fnllabatt = Integer.parseInt(num.readLine());
p.print("Enter Final Compilation grade: ");
int fnllabbehave = Integer.parseInt(num.readLine());
p.print("Enter Final Seatwork grade: ");
int fnllabseatw = Integer.parseInt(num.readLine());
