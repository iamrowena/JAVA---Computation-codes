TOTALFLABEXAM);
p.println(" Total Final Laboratory Grade:"+TOTALFLABGRADE);p.println("");

double prelim = (TOTALPGRADE*.55) + (TOTALPLABGRADE*.45);
p.println("Total Prelim Grade is "+prelim);
double mid = (TOTALMGRADE*.55) + (TOTALMLABGRADE*.45);
p.println("Total Midterm Grade is "+mid);
double fnl = (TOTALFGRADE*.55) + (TOTALFLABGRADE*.45);

p.println("Total Final Grade is "+fnl);p.println("");

double totalpmf = (prelim*.30) + (mid*.30) + (fnl*.40);
p.println("Total Overall Grade is "+totalpmf);
